    "cssnano": "4.1.10",
    "cz-customizable": "6.2.0",
    "doiuse": "4.2.0",
    "eslint": "6.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "extract-css-chunks-webpack-plugin": "4.5.6",
    "file-loader": "4.1.0",
    "generate-robotstxt": "7.1.0",
    "glob": "7.1.4",
    "gray-matter": "4.0.2",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-data": "1.3.1",
    "gulp-favicons": "2.3.1",
    "gulp-footer": "2.0.2",
    "gulp-hash-filename": "2.0.1",
    "gulp-if": "3.0.0",
    "gulp-ignore": "3.0.0",
    "gulp-jsbeautifier": "3.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-pug": "4.0.1",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-responsive": "2.14.0",
    "gulp-sass": "4.0.2",
    "highlight.js": "9.15.8",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "3.0.1",
    "image-size": "0.7.4",
    "lint-staged": "9.2.0",
    "loader-utils": "1.2.3",
    "markdown-it": "9.0.1",
    "markdown-it-anchor": "5.2.4",
    "markdown-it-attrs": "2.4.1",
    "markdown-it-container": "2.0.0",
    "markdown-it-fontawesome": "0.3.0",
    "markdown-it-footnote": "3.0.2",
    "markdown-it-header-sections": "1.0.0",
    "markdown-it-multimd-table": "3.2.0-rc.1",
    "markdown-it-sub": "1.0.0",
    "markdown-it-sup": "1.0.0",
    "node-sass": "",
    "node-sass-tilde-importer": "1.0.2",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss": "7.0.17",
    "postcss-easing-gradients": "3.0.1",
    "postcss-easings": "2.0.0",
    "postcss-font-magician": "2.2.2",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-reporter": "6.0.1",
    "postcss-short": "5.0.0",
    "postcss-sorting": "5.0.1",
    "prettier": "1.18.2",
    "prettier-loader": "2.1.1",
    "pug": "3.0.1",
    "responsive-loader": "1.2.0",
    "sass-loader": "7.1.0",
    "schema-utils": "2.0.1",
    "sqip": "1.0.0-alpha.26",
    "sqip-plugin-blur": "1.0.0-alpha.27",
    "sqip-plugin-data-uri": "1.0.0-alpha.27",
    "sqip-plugin-primitive": "1.0.0-alpha.27",
    "sqip-plugin-svgo": "1.0.0-alpha.27",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-order": "3.0.1",
    "stylelint-scss": "3.9.2",
    "stylemark": "3.1.3",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "2.1.0",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-middleware": "3.7.0",
    "webpack-fix-style-only-entries": "0.3.0",
    "webpack-hot-middleware": "2.25.0",
    "webpackbar": "3.2.0",
    "xo": "0.24.0"
  },
  "browserslist": [
    "ie >= 10",
    "> 1%",
    "not OperaMini all"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,md,scss}": [
      "prettier",
      "xo",
      "git add"
    ]
  },
  "scripts": {
    "dev": "NODE_ENV=development gulp dev --cwd ./src/ --gulpfile ./scripts/gulp",
    "style": "NODE_ENV=development gulp styleguide --cwd ./src/ --gulpfile ./scripts/gulp",
    "build": "NODE_ENV=production gulp prod --cwd ./src/ --gulpfile ./scripts/gulp"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "space": true,
    "semicolon": false,
    "prettier": true
  }
}
